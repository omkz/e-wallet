FORM Withdrawal:
<%= form_with model: @withdraw, url: "/transactions/withdraws",local: true, method: "post" do |f| %>
  <div class="control">
    <%= f.label 'amount', class: "label" %>
    <%= f.text_field :amount, class: "input" %>
  </div>
  <%= f.submit :post, class: 'button is-primary' %>
<% end %>




